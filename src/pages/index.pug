doctype html
html(lang="en")
  head
    title Title
    meta(charset="UTF-8")
    if meta
      each item in meta
        meta(name=item.name, content=item.content)
    link(rel="stylesheet", href="fonts/bundle.css")
    link(rel="stylesheet", href="css/main.css")
  body(style="overflow: hidden")
    main.index-menu
      h3 Список страниц в проекте1
      ol
        each page in pages
          li
            a(href=page)=page
    script(src="js/main.js")
